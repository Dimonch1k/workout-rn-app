{
	"expo": {
		"name": "workout-rn-app",
		"slug": "workout-rn-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "",
		"scheme": "workout-rn-app",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"userInterfaceStyle": "automatic"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"userInterfaceStyle": "automatic"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": ""
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
